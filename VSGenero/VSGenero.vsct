      <Button guid="guidVSGeneroCmdSet" id="cmdidExtractSqlStatements" priority="0x0000" type="Button">
        <CommandFlag>DynamicVisibility</CommandFlag>
        <Strings>
          <ButtonText>Extract SQL Statement(s)</ButtonText>
