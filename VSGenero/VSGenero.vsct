    <GuidSymbol name="guidVSGeneroPkg" value="{18274d18-91c9-420c-a121-2ffe4f920b4e}"/>
    <GuidSymbol name="guidVSGeneroCmdSet" value="{730fb573-e079-4739-aec6-45c02012ac76}">
